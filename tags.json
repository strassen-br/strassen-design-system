{
  "st-button": {
    "description": "Square uppercase button",
    "attributes": [
      "kind",
      "color",
      "label",
      "disabled",
      "click"
    ],
    "defaults": [
      "kind",
      "color",
      "click"
    ]
  },
  "st-link": {
    "description": "Clickable simple text link",
    "attributes": [
      "label",
      "color",
      "size",
      "underline-behavior",
      "disabled",
      "uppercase",
      "click"
    ],
    "defaults": [
      "label",
      "click"
    ]
  },
  "st-notification-group": {
    "description": "Wrapper for notifications generated when calling $notify",
    "attributes": [
      "group",
      "position",
      "duration-in-ms",
      "close-on-click",
      "ignore-duplicates"
    ]
  },
  "st-text-input": {
    "description": "Base text input",
    "attributes": [
      "top-label-text",
      "bottom-label-text",
      "value",
      "color",
      "size",
      "disabled",
      "uppercase",
      "click",
      "input"
    ],
    "defaults": [
      "topLabelText"
    ]
  },
  "st-icon": {
    "description": "Remix Icon",
    "attributes": [
      "name",
      "kind",
      "click"
    ],
    "defaults": [
      "name",
      "kind"
    ]
  },
  "st-loading-indicator": {
    "description": "Block display small loading indicator",
    "attributes": [],
    "defaults": []
  }
}